{
  "name": "js-oauth2",
  "version": "2.0.19",
  "description": "OAuth2 authentication module written in vanilla ES6",
  "keywords": [
    "oauth2",
    "webpack",
    "browserify",
    "fetch",
    "react"
  ],
  "license": "MIT",
  "author": "Bruno Carvalho de Araujo <brunocarvalhodearaujo@gmail.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/brunocarvalhodearaujo/js-oauth2.git",
    "type": "git"
  },
  "scripts": {
    "test": "jest --runInBand --no-cache --forceExit --coverage",
    "build": "tsc -d",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/lodash.omit": "^4.5.6",
    "events": "^3.2.0",
    "fetch-intercept": "^2.3.1",
    "lodash.omit": "^4.5.0",
    "querystring": "^0.2.0"
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!src/**/index.{ts,js}",
      "!src/**/*.{test,spec}.{ts,js}"
    ],
    "testRegex": "(test|src|__test__)/.*\\.(spec|test)\\.(js|ts)$",
    "coverageThreshold": {
      "global": {
        "statements": 75,
        "branches": 75,
        "functions": 75,
        "lines": 75
      }
    }
  }
}
