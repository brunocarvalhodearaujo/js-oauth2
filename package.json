{
  "name": "js-oauth2",
  "version": "2.0.18",
  "description": "OAuth2 authentication module written in vanilla ES6",
  "keywords": [
    "oauth2",
    "webpack",
    "browserify",
    "nodejs",
    "react",
    "authentication"
  ],
  "license": "MIT",
  "author": "Bruno Carvalho de Araujo <brunocarvalhodearaujo@gmail.com>",
  "main": "lib/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "jest --coverage --runInBand --no-cache --forceExit",
    "prepublish": "babel src --out-dir lib"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-inline-imports-commonjs": "1.2.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "jest": "23.6.0"
  },
  "dependencies": {
    "events": "3.0.0",
    "fetch-everywhere": "1.0.5",
    "fetch-intercept": "2.3.1",
    "lodash.omit": "4.5.0",
    "querystring": "0.2.0"
  },
  "repository": {
    "url": "https://github.com/brunocarvalhodearaujo/js-oauth2.git",
    "type": "git"
  },
  "jest": {
    "verbose": true,
    "coverageReporters": [
      "text-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  }
}
